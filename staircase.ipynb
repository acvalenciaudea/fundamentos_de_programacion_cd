   "source": [
    "# 3. Staircase\n",
    "1. n: size of stair"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c0892a85",
   "metadata": {},
   "outputs": [],
   "source": [
    "def staircase(n):\n",
    "    result = \"\"\n",
    "    for i in range(n):\n",
    "        result += \" \" * (n - (i + 1)) + \"#\" * (i + 1) + \"\\n\"\n",
    "\n",
    "    print(result)\n",
    "    return result\n",
    "\n"
   ]
